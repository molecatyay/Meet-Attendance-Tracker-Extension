{  
  "manifest_version": 3,
  "name": "Meet Attendance Bot",
  "description": "Collect Attendance in google meet!",
  "version": "1.0",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["/content.js"]
  }],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "storage"
  ],
  "options_page": "options/options.html"
}